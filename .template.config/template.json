{
    "$schema": "http://json.schemastore.org/template",
    "author": "Kashif Jamal Soofi",
    "classifications": [ "WebApi" ],
    "name": "Template Api",
    "identity": "Template.Api.CSharp",
    "groupIdentity": "Template.Api",
    "shortName": "mytemplate",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "exclude": [ ".vs/**/*", "**/[Bb]in/**", "**/[Oo]bj/**", ".template.config/**/*", "**/*.lock.json" ],
    "symbols": {
        "name": {
            "type": "parameter",
            "datatype": "string",
            "required": true,
            "replaces": "Template",
            "fileRename": "Template"
        }
    }
}